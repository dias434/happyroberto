{
  "name": "@happyroberto/web",
  "private": true,
  "scripts": {
    "install:web": "npm --prefix apps/web install",
    "build": "next build",
    "dev": "npm run dev:web",
    "lint": "npm --prefix apps/web run lint",
    "start": "npm --prefix apps/web run start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "node scripts/ensure-prisma-env.mjs --required && prisma migrate dev",
    "prisma:deploy": "node scripts/ensure-prisma-env.mjs --required && prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "next": "14.2.14",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.14",
    "prisma": "5.19.1",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
